{
  "name": "safe-read-write",
  "version": "1.0.0",
  "description": "Uses lockfile to write and read simple text files",
  "main": "index.js",
  "scripts": {
    "dev": "npm run compile -- --watch",
    "build": "rm -rf lib && npm run compile",
    "compile": "babel src -d lib",
    "test": "mocha --require babel-register src/test/**/*.spec.js",
    "tdd": "npm test -- -w"
  },
  "keywords": [
    "lock",
    "lockfile",
    "promise",
    "request",
    "then",
    "thenable"
  ],
  "author": "Jorge Bastias (jbastias)",
  "license": "ISC",
  "dependencies": {
    "lockfile": "^1.0.3",
    "mz": "^2.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "randomstring": "^1.1.5"
  }
}
